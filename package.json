{
  "name": "PhotoMind",
  "appId": "com.photomind.app",
  "productName": "PhotoMind",
  "version": "1.0.0",
  "description": "智能图片管理系统 - 自然语言搜索 iCloud 照片",
  "author": "PhotoMind",
  "license": "MIT",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "tsc -p tsconfig.electron.json && node fix-imports.js",
    "build": "vite build && electron-builder",
    "build:mac": "vite build && electron-builder --mac --x64",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vicons/fluent": "^0.13.0",
    "@xenova/transformers": "^2.17.2",
    "dayjs": "^1.11.10",
    "esbuild": "^0.27.2",
    "naive-ui": "^2.38.0",
    "openai": "^4.28.0",
    "pinia": "^2.1.7",
    "sharp": "^0.33.0",
    "sql.js": "^1.10.0",
    "uuid": "^9.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "electron": "^35.0.0",
    "electron-builder": "^24.13.0",
    "happy-dom": "^20.5.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^1.8.0"
  },
  "build": {
    "productName": "PhotoMind",
    "appId": "com.photomind.app",
    "copyright": "Copyright © 2024 PhotoMind",
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist-electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.photography",
      "target": "default",
      "icon": null
    },
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "npmRebuild": false
  }
}
